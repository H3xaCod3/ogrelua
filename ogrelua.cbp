<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<CodeBlocks_project_file>
	<FileVersion major="1" minor="6" />
	<Project>
		<Option title="ogrelua" />
		<Option pch_mode="2" />
		<Option compiler="gcc" />
		<Build>
			<Target title="Debug">
				<Option output="bin/Debug/ogrelua" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Debug/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-Wall" />
					<Add option="-g" />
					<Add option="-DBUILD_DLL" />
				</Compiler>
			</Target>
			<Target title="Release">
				<Option output="bin/Release/ogrelua" prefix_auto="1" extension_auto="1" />
				<Option object_output="obj/Release/" />
				<Option type="3" />
				<Option compiler="gcc" />
				<Option createDefFile="1" />
				<Option createStaticLib="1" />
				<Compiler>
					<Add option="-O3" />
					<Add option="-Wall" />
					<Add option="-DBUILD_DLL" />
				</Compiler>
				<Linker>
					<Add option="-s" />
				</Linker>
			</Target>
		</Build>
		<Compiler>
			<Add option="-std=c++11" />
			<Add directory="$(OGRE_HOME)/include" />
			<Add directory="$(OGRE_HOME)/include/OGRE" />
			<Add directory="$(OGRE_HOME)/include/OIS" />
			<Add directory="$(LUA_HOME)/src" />
		</Compiler>
		<Linker>
			<Add library="$(OGRE_HOME)\lib\release\libOgreTerrain.dll.a" />
			<Add library="$(OGRE_HOME)\lib\release\libOgreMain.dll.a" />
			<Add library="$(LUA_HOME)\src\lua51.dll" />
		</Linker>
		<Unit filename="FrameListener.cpp" />
		<Unit filename="FrameListener.h" />
		<Unit filename="main.cpp" />
		<Extensions>
			<code_completion />
			<envvars />
			<debugger />
			<lib_finder disable_auto="1" />
		</Extensions>
	</Project>
</CodeBlocks_project_file>
